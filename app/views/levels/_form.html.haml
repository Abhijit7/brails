= simple_nested_form_for @level, html: {:class => 'form-horizontal'} do |f|
  = f.error_notification

  .form-inputs
    = f.input :level_number
    = f.input :level_name
    = f.simple_fields_for :bonus_round do |b|
      = b.input :extra_cookies

  .form-actions
    = f.button :submit, class: 'btn-success'
